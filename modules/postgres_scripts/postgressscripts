CREATE TABLE employee (
	user_id INT NOT NULL,
	full_name VARCHAR ( 50 )  NOT NULL,
	email VARCHAR ( 255 )  NOT NULL,
	created_on TIMESTAMP NOT NULL,
        last_login TIMESTAMP 
);

